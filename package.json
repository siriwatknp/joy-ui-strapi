    "husky": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "size-limit": "8.0.1",
    "tsdx": "0.14.1",
    "tslib": "2.4.0",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
